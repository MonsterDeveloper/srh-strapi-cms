{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "description": {
      "type": "text"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "startTime": {
      "type": "datetime",
      "required": false
    },
    "endTime": {
      "type": "datetime"
    },
    "eventType": {
      "type": "enumeration",
      "enum": [
        "movie",
        "concert",
        "exhibition",
        "theater",
        "workshop",
        "conference"
      ]
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "maxCap": {
      "type": "integer"
    },
    "website": {
      "type": "string"
    },
    "tags": {
      "type": "json"
    },
    "seatMaps": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "languages": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::language.language",
      "inversedBy": "events"
    },
    "location": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::location.location",
      "inversedBy": "events"
    },
    "organizer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::organizer.organizer",
      "inversedBy": "events"
    },
    "tickets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ticket.ticket",
      "mappedBy": "event"
    }
  }
}
