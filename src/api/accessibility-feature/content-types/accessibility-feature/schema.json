{
  "kind": "collectionType",
  "collectionName": "accessibility_features",
  "info": {
    "singularName": "accessibility-feature",
    "pluralName": "accessibility-features",
    "displayName": "Accessibility Feature",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "mobility",
        "visual",
        "hearing",
        "cognitive",
        "sensory",
        "communication"
      ]
    },
    "tickets": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::ticket.ticket",
      "mappedBy": "accessibilityFeatures"
    },
    "locations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::location.location",
      "mappedBy": "accessibilityFeatures"
    },
    "icon": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    }
  }
}
